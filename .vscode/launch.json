	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "http2 server",
			"console": "integratedTerminal",
			"program": "${workspaceFolder}\\dist\\server\\index.js",
			"outFiles": [
				"${workspaceFolder}/dist/**/*.js"
			],
			"args": [
				"--config",
				"${workspaceFolder}\\config.yml"
			],
			"runtimeArgs": [
				"--experimental-modules",
				"--es-module-specifier-resolution",
				"node"
			]
		}
	]
}