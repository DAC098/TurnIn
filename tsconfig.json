{
    "compilerOptions": {
        "module": "esnext",
		"moduleResolution": "node",
		"esModuleInterop": true,
        "target":"esnext",
        "sourceMap": true,
		"jsx": "react",
		"outDir": "./dist",
        "types": [
            "node",
            "react",
            "react-dom",
            "classnames",
            "styled-components"
        ],
		"allowSyntheticDefaultImports": true
    },
    "include": [
		"src"
	]
}